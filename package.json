{
  "name": "in-nutri-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/init-db.ts",
    "db:seed-data": "tsx -e \"import { seedData } from './db/seedData'; seedData().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); });\"",
    "db:migrate-d1": "wrangler d1 execute in-nutri-db --file=./drizzle/0000_secret_the_initiative.sql",
    "deploy:cf": "npm run pages:build && wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.1",
    "adminjs": "^7.8.17",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.6",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.555.0",
    "next": "^15.5.2",
    "qrcode.react": "^4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.5.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
